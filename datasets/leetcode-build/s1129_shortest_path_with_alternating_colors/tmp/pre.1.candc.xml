<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="ba" cat="N">
     <rule type="fa" cat="N">
      <lf start="1" span="1" word="arrays" lemma="array" pos="NNS" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="2" span="1" word="param_redEdges" lemma="param_redEdges" pos="NNS" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="conj" cat="N\N">
      <lf start="3" span="1" word="and" lemma="and" pos="CC" chunk="I-NP" entity="O" cat="conj" />
      <lf start="4" span="1" word="param_blueEdges" lemma="param_blueEdges" pos="NNS" chunk="I-NP" entity="I-ORG" cat="N" />
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <rule type="bx" cat="(S[dcl]\NP)/(S[adj]\NP)">
     <lf start="5" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
     <lf start="6" span="1" word="not" lemma="not" pos="RB" chunk="O" entity="O" cat="(S\NP)\(S\NP)" />
    </rule>
    <lf start="7" span="1" word="null" lemma="null" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
