for entry in `find ./datasets/leetcode-build/s$1* -type f -name "*.mr" | sort`
do
    filename=`echo $entry | sed 's/.*\///'`
    index=`echo $filename | sed 's/\..*//'`
    folder=`echo $entry | sed "s/\/$filename//"`
    echo "./bin/main -f $entry -s ./specs/si/std_si_2023.yml,$folder/dynamic_si.yml"    
    ./bin/main -f $entry -s ./specs/si/std_si_2023.yml,$folder/dynamic_si.yml
    echo '----------------------------------'
done